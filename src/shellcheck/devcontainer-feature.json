{
    "id": "shellcheck",
    "version": "0.2.3",
    "name": "Shellcheck",
    "description": "A static analysis tool for shell scripts",
    "options": {
        "version": {
            "type": "string",
            "description": "version of shellcheck to install",
            "proposals": [
                "stable",
                "v0.8.0",
                "v0.7.2"
            ],
            "default": "stable"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "timonwong.shellcheck"
            ]
        }
    }
}
